{
  "expo": {
    "name": "ColorBlinder",
    "slug": "color-blinder",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "React Native game developed using Expo and TypeScript.\n\nBased on the tutorial series https://blog.risingstack.com/a-definitive-react-native-guide-for-react-developers/",
    "githubUrl": "https://github.com/sjimenez77/colorblinder"
  }
}
